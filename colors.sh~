# Swap to Dark if you have to.
function setColors {
if [ $1 == "dark" ]; then
    S_base03=#002b36
    S_base02=#073642
    S_base01=#586e75
    S_base00=#657b83
    S_base0=#839496
    S_base1=#93a1a1
    S_base2=#eee8d5
    S_base3=#fdf6e3
fi
}

function generateGtk {
    OOMOXCONF="/opt/oomox/colors/xresources/xresources"

    # make the theme
#    oomox-cli $OOMOXCONF
    OOMOXTHEME="oomox-xresources"
}
